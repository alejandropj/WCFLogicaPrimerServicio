<?xml version="1.0"?>
<configuration>
	<system.serviceModel>
		<services>
			<service name="LOGICA.CLASS"
								 behaviorConfiguration="openWSDL">
				<endpoint address=""
										  binding="basicHttpBinding"
										  contract="LOGICA.CONTRACT"/>
			</service>
		</services>
		<behaviors>
			<serviceBehaviors>
				<behavior name="openWSDL">
					<serviceMetadata httpGetEnabled="true"/>
